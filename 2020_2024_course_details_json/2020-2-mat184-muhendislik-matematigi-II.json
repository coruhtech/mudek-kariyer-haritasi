  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Üç boyutlu uzayda vektörler, doğrular, düzlemler ve farklı koordinat sistemleri (Kartezyen, silindirik, küresel) ile çalışır; vektör değerli fonksiyonların limit, türev ve integralini hesaplar."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Çok değişkenli fonksiyonların limit, süreklilik, kısmi türev, zincir kuralı, yönlü türev ve gradyan kavramlarını anlar ve hesaplar."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "Çok değişkenli fonksiyonların ekstremum değerlerini (yerel/mutlak) bulur ve Lagrange çarpanları yöntemiyle kısıtlı optimizasyon problemlerini çözer."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "İki ve üç katlı integralleri farklı koordinat sistemlerinde hesaplar ve alan, hacim, kütle gibi fiziksel/geometrik büyüklüklerin hesaplanmasında uygular."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "Vektör alanlarının diverjans ve rotasyonelini hesaplar; eğrisel integralleri (çizgi integralleri) tanımlar ve hesaplar."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "Yüzey integrallerini tanımlar ve hesaplar; Green, Stokes ve Diverjans (Gauss) teoremlerini bilir ve uygular."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Çok değişkenli analiz ve vektör kalkülüs kavramlarını diğer mühendislik derslerindeki problemlerin modellenmesi ve çözümü için bir temel olarak kullanır."
    }
  ],
