  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Önermeler mantığı (doğruluk tabloları, mantıksal denklikler) ve niceleyicileri kullanarak matematiksel ifadeleri formüle eder; doğrudan/dolaylı ispat, tümevarım gibi ispat yöntemlerini uygular."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Kümeler, küme işlemleri, fonksiyonlar (birebir, örten) ve bağıntıları (denklik, sıralama) tanımlar ve bu kavramları problem çözümünde kullanır."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "Temel graf teorisi kavramlarını (yollar, devreler, bağlantılılık) ve ağaç yapılarını (özellikleri, dolaşma) bilir; basit graf algoritmalarını açıklar."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "Temel sayma prensiplerini (çarpma, toplama), permütasyon, kombinasyon ve güvercin yuvası ilkesini kullanarak kombinatorik problemlerini çözer."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "Temel cebirsel yapıları (gruplar, halkalar, cisimler) ve Boole cebri/kafes yapılarını tanımlar ve özelliklerini analiz eder."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "Yineleme bağıntılarını kurar ve basit olanları çözer; sonlu durumlu makinelerin çalışma prensibini anlar."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Ayrık matematik kavramlarını (mantık, kümeler, bağıntılar, graflar vb.) bilgisayar bilimlerindeki algoritmaların analizi, veri yapılarının modellenmesi gibi alanlarda kullanır."
    }
  ],
