  "dersIcerik": {
    "dersinAmaci": "Bu dersin temel amacı, öğrencilerin yönetim bilimi ve işletme yönetimi ile ilgili temel kavramları anlamalarını sağlamak, yönetim süreçlerini (planlama, örgütleme, yöneltme, denetim) kavramalarını ve modern yönetim yaklaşımları hakkında bilgi sahibi olmalarını sağlamaktır.",
    "dersinOnKosuluOlanDersler": "Mevcut Değil",
    "dersinIcerigi": "Yönetim ve organizasyon kavramları, yönetim teorileri (klasik, neoklasik, modern), yönetim fonksiyonları (planlama, örgütleme, yöneltme, koordinasyon ve denetim), karar verme süreçleri, motivasyon teorileri, liderlik yaklaşımları, stratejik yönetim, işletmelerde etik ve sosyal sorumluluk.",
    "dersinIcinOnerilenHususlar": "Öğrencilerin temel sosyal bilimler kavramlarına aşina olması ve güncel iş dünyası örneklerini takip etmesi önerilir.",
    "ogretimTuru": "Birinci Öğretim",
    "stajDurumu": "Mevcut Değil",
    "dersinKitabiMalzemesiOnerilenKaynaklar": "İlgili ders notları ve öğretim üyesinin önereceği kaynaklar.",
    "dersiVerenOgretimUyesiOgretimGorevlisi": "",
    "hazirlanmaTarihi": "27.04.2025"
  },
  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Yönetimin temel kavramlarını, amaçlarını, önemini ve işletme içindeki temel fonksiyonlarını (planlama, örgütleme, yöneltme, koordinasyon, denetim) açıklar."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Yönetim düşüncesinin tarihsel gelişimini bilir; klasik, neoklasik ve modern yönetim teorilerini ve yaklaşımlarını (sistem, durumsallık vb.) karşılaştırır."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "İşletmelerde planlama, örgütleme (yapı türleri dahil), koordinasyon ve denetim süreçlerini analiz eder."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "Yöneltme fonksiyonu kapsamında motivasyon teorilerini ve liderlik yaklaşımlarını açıklar."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "İşletmelerde karar verme süreçlerini anlar ve temel stratejik yönetim kavramlarını bilir."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "İşletme etiği ve sosyal sorumluluk kavramlarının önemini ve işletme yönetimi üzerindeki etkilerini değerlendirir."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Yönetim bilimi prensiplerini güncel iş dünyası problemlerini anlamak ve analiz etmek için kullanır."
    }
  ],
