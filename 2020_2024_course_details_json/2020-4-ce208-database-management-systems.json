  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Veritabanı sistemlerinin temel kavramlarını, mimarisini, avantajlarını ve VTYS işlevlerini açıklar."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Veritabanı tasarımı için gereksinim analizi yapar; Varlık-Bağıntı (ER) ve genişletilmiş ER modelleri kullanarak kavramsal tasarımı gerçekleştirir."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "İlişkisel modeli anlar, normalizasyon prensiplerini uygular ve kavramsal modeli mantıksal tasarıma dönüştürür."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "SQL dilinin veri tanımlama (DDL) ve veri işleme (DML) komutlarını kullanarak veritabanı şeması oluşturur ve verileri sorgular, ekler, siler, günceller."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "İndeksleme, tetikleyiciler (triggers) ve saklı yordamlar (stored procedures) gibi ileri VTYS özelliklerini anlar ve uygular."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "Veri bütünlüğü kısıtlamalarını tanımlar ve uygular; kullanıcı yetkilendirme, yedekleme ve geri yükleme gibi temel veritabanı yönetimi işlemlerini gerçekleştirir."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Belirli bir probleme yönelik veritabanı uygulamasını tasarlamak için modern VTYS araçlarını (örn: MySQL) ve modelleme tekniklerini (örn: UML) kullanır."
    }
  ],
