  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Üniversitede edindiği temel mühendislik (matematik, fen bilimleri, bilgisayar) bilgilerini ve problem çözme becerilerini staj yaptığı kurumdaki pratik uygulamalarda kullanır."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Çalışma ortamındaki profesyonellerle (mühendisler, yöneticiler, ekip üyeleri) sözlü ve yazılı etkin iletişim kurar, talimatları anlar ve verir."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "Bilgisayar mühendisliği alanındaki modern teknikleri, araçları ve bilişim teknolojilerini kullanarak verilen görevleri yerine getirir ve gerçek dünya problemlerine çözümler geliştirir/uyarlar."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "Staj sürecinde edindiği teknik bilgileri, deneyimleri ve gözlemleri belirlenen standartlara uygun, yapılandırılmış bir staj raporu halinde sunar."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "İşyeri disiplinine ve kurallarına uyar, verilen sorumlulukları alır, bireysel veya takım içinde etkin çalışma sergiler."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "Staj yaptığı kurumun çalışma alanı, organizasyon yapısı ve iş süreçleri hakkında bilgi edinir; mühendislik uygulamalarının toplumsal ve evrensel boyutlarını gözlemler."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Mesleki etik ilkelerinin iş hayatındaki yansımalarını gözlemler ve mesleki sorumluluk bilinci geliştirir."
    }
  ],
