  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Bilgisayar mimarisinin temel kavramlarını, bileşenlerini (CPU, bellek, G/Ç), gelişimini ve performans ölçütlerini açıklar."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Merkezi İşlem Birimi'nin (CPU) tasarımını, veri yolunu ve kontrol birimini (tek/çok vuruşlu, mikroprogramlama) analiz eder ve temel tasarım prensiplerini uygular."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "MIPS gibi bir buyruk kümesi mimarisinin (ISA) komutlarını, veri formatlarını ve adresleme modlarını kullanarak assembly dilinde basit programlar yazar."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "RISC/CISC mimarilerinin temel prensiplerini karşılaştırır ve MIPS gibi örnek bir RISC mimarisini analiz eder."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "Boru hattı (pipelining), önbellek optimizasyonu ve komut/veri/thread seviyesi paralelleştirme gibi CPU performansını artıran teknikleri ve bunların performans metriklerine etkisini analiz eder."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "Önbellek (cache) ve sanal bellek gibi bellek hiyerarşisi bileşenlerinin çalışma prensiplerini ve performans etkilerini açıklar."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Çok işlemcili sistemler ve GPU mimarileri gibi modern mimari yaklaşımları hakkında temel bilgi sahibi olur."
    }
  ],
