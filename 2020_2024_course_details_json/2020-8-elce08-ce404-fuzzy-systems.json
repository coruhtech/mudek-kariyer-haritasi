  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Bulanık kümelerin, bulanık mantığın ve üyelik fonksiyonlarının temel kavramlarını açıklar ve bu kavramların geleneksel yöntemlerle çözülemeyen karmaşık mühendislik problemlerindeki rolünü analiz eder."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Bulanık kümeler ve bulanık ilişkiler üzerinde matematiksel işlemleri uygular, sonuçları yorumlar ve farklı uygulama alanlarında bu işlemlerin kullanımını değerlendirir."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "Bulanık kural tabanlı çıkarım sistemlerini (Mamdani ve Sugeno tipleri) tasarlar, analiz eder ve tasarım sürecinde etik ilkelere ve standartlara uygun kararlar verir."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "Uygun araçlar veya programlama dilleri kullanarak bulanık sistemleri uygular, süreç ve sonuçları açık ve anlaşılır şekilde raporlar ve sunar."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "Kontrol veya karar verme gibi alanlarda belirsizlik içeren problemleri çözmek için bulanık mantık tekniklerini uygular ve bu tekniklerin toplumsal ve endüstriyel etkilerini değerlendirir."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "Bulanık sistem projelerinde takım çalışması gerektiren problem çözme yöntemlerini uygular, uluslararası standartları takip ederek ve farklı disiplinlerden uzmanlarla işbirliği yaparak çözümler geliştirir."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Bulanık mantık ve sistemler alanındaki yeni teknolojileri ve trendleri araştırır, yaşam boyu öğrenme yaklaşımıyla bu teknolojilerin etik ve sürdürülebilir uygulamalarını geliştirir."
    }
  ],
