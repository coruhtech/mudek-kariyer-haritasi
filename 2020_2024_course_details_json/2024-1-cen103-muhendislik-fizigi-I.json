  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Temel fiziksel nicelikleri (uzunluk, kütle, zaman), birimleri (SI) ve vektörleri kullanarak tek ve iki boyutta kinematik problemlerini matematiksel olarak formüle eder ve çözer."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Newton'un hareket yasalarını kavrar ve parçacık dinamiği problemlerine (sürtünme dahil) uygular."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "İş, kinetik enerji, potansiyel enerji ve güç kavramlarını tanımlar; iş-enerji teoremini ve mekanik enerjinin korunumu ilkesini problem çözümünde kullanır."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "Çizgisel momentum, itme kavramlarını ve momentumun korunumu ilkesini açıklar; bir ve iki boyutta çarpışma problemlerini analiz eder."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "Katı cisimlerin sabit bir eksen etrafında dönme kinematiğini ve dinamiğini (tork, açısal momentum, eylemsizlik momenti) analiz eder."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "Statik denge koşullarını rijit cisimlere uygular; basit harmonik hareket kavramını açıklar."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Temel akışkanlar mekaniği (basınç, kaldırma kuvveti, akışkan dinamiği) ve evrensel kütle çekim yasası ile ilgili problemleri çözer."
    },
    {
      "id": "ÖÇ.8",
      "aciklama": "Klasik mekaniğin temel prensiplerini diğer fen ve mühendislik alanlarındaki (özellikle Bilgisayar Mühendisliği) problemlerin modellenmesi ve çözümü için gerekli temel bilgiyi edinir."
    }
  ],
