  "dersOgrenmeÇiktilari": [
    {
      "id": "ÖÇ.1",
      "aciklama": "Eğitim psikolojisinin temel kavramlarını, araştırma yöntemlerini ve mühendislik bağlamında öğrenme süreçlerinin önemini açıklar."
    },
    {
      "id": "ÖÇ.2",
      "aciklama": "Bilişsel, duyuşsal ve sosyal gelişim teorilerini (Piaget, Vygotsky, Erikson, Kohlberg) bilir ve bu süreçlerin bireyin öğrenme ve problem çözme yaklaşımları üzerindeki etkilerini analiz eder."
    },
    {
      "id": "ÖÇ.3",
      "aciklama": "Temel öğrenme yaklaşımlarını (davranışçı, bilişsel, yapılandırmacı) ve motivasyon teorilerini karşılaştırarak, etkili öğrenme ve öğretme stratejileri geliştirmedeki rollerini değerlendirir."
    },
    {
      "id": "ÖÇ.4",
      "aciklama": "Bireysel farklılıkların (zeka, kişilik, öğrenme stilleri) öğrenme ve takım çalışması üzerindeki etkilerini anlar ve etik ilkeler çerçevesinde değerlendirir."
    },
    {
      "id": "ÖÇ.5",
      "aciklama": "Eğitimde ölçme ve değerlendirme yaklaşımlarını bilir ve öğrenme süreçlerinin değerlendirilmesindeki önemini kavrar."
    },
    {
      "id": "ÖÇ.6",
      "aciklama": "Sınıf yönetimi veya grup dinamiği stratejilerini bilir ve işbirliğine dayalı öğrenme/çalışma ortamları oluşturmadaki uygulamalarını değerlendirir."
    },
    {
      "id": "ÖÇ.7",
      "aciklama": "Eğitim psikolojisi prensiplerini, kendi öğrenme süreçlerini iyileştirmek ve başkalarıyla (örn: kullanıcılar, ekip üyeleri) etkileşimde bulunmak için kullanır."
    }
  ],
